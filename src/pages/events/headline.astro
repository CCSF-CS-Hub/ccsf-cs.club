---
import Layout from "../../layouts/Layout.astro";
---

<Layout title="Industry Speaker Event">
  <main>
    <div class="flex flex-col h-screen items-center justify-center py-4">
      <div class="w-full h-full max-w-screen-lg max-h-screen-lg">
        <object
          src="/2024_03_13_Tech_Panel_Flyer.pdf"
          class="h-full w-full"
        >
          <embed class="h-full w-full" src="https://drive.google.com/file/d/1mkgyc1EoFvDk3Z2QZr1NN5AHlLc3VRz_/preview?usp=sharing" width="100%" height="600px"/>
        </object>
      </div>
      <p class="p-8 text-center">
        Come listen to a speaker panel of seasoned tech veterans!
      </p>
    </div>
  </main>
</Layout>
